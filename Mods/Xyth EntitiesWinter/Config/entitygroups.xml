<configs>
  <append xpath="/entitygroups/entitygroup[ starts-with(@name, 'FriendlyAnimals')]">
	<entity name="SealCub" prob="0.5" />
  </append>

  <append xpath="/entitygroups/entitygroup[ starts-with(@name, 'EnemyAnimals')]" >
	<entity name="DeadSnake" prob="1" />
	<entity name="ArcticFox" prob="0.5" />
	<entity name="Polarbear" prob="0.25" />
  </append>
  
  <append xpath="/entitygroups/entitygroup[@name='ZombiesNight']">
	<entity name="Abominable" prob="1" />
  </append>

  <append xpath="/entitygroups/entitygroup[@name='EnemyAnimalsForest']">
	<entity name="MawMec" prob="0.1" />
  </append>
  
  <append xpath="/entitygroups/entitygroup[@name='ZombiesAll']"> 
	<entity name="Snowman" prob="2" />
	<entity name="Snowcop" prob="1" />
  </append>
	
</configs>