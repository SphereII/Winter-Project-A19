<configs>
  <append xpath="/entitygroups/entitygroup[ (contains( @name, 'ombies')) or @name='HospitalGroup' or (contains(@name, 'HordeStage')) ]">
    <entity name="zombieBurnt_white"/>
    <entity name="zombieDemolition_white"  prob="0.3"/>
    <entity name="zombieFarmer_white"/>
    <entity name="zombieFatCop_white" prob="0.3"/>
    <entity name="zombieFatHawaiian_white"/>
    <entity name="zombieFootballPlayer_white"  prob="0.3"/>
    <entity name="zombieMaleHazmat_white"/>
    <entity name="zombieSkateboarder_white"/>
    <entity name="zombieLab_white"/>
    <entity name="zombieNurse_white"/>
    <entity name="zombieOldTimer_white"/>
    <entity name="zombieSnow_white"/>
    <entity name="zombieSoldier_white"/>
    <entity name="zombieSpider_white"/>
  </append>

  <insertBefore xpath="//entity[@name='zombieWightFeral']">
    <entity name="zombieWightFeral_white"/>
  </insertBefore>
  
  <insertBefore xpath="//entity[@name='zombieScreamer']">
    <entity name="zombieScreamer_white"/>
  </insertBefore>
  <insertBefore xpath="//entity[@name='animalWolf']">
    <entity name="animalZombieWolf_white"/>
  </insertBefore>
  <insertBefore xpath="//entity[@name='animalZombieDog']">
    <entity name="animalZombieDog_white" />
  </insertBefore>
</configs>